{
    "extends": ["standard",
        "plugin:react/recommended",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "react/prop-types": "off",
        "prettier/prettier": [
          "error",
          {
            "semi": true
          }
        ]
    }
}
